{
  "name": "fullstack-sandbox",
  "version": "1.0.0",
  "description": "Just a sandbox for getting into fullstack development",
  "main": "src/app.js",
  "type": "module",
  "config": {
    "entry": "./src/app.js",
    "node_dev_options": "--trace-sync-io"
  },
  "scripts": {
    "start": "node $npm_package_config_entry",
    "start:dev": "node $npm_package_config_entry $npm_package_config_node_dev_options",
    "debug": "DEBUG=fullstack-sandbox npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src"
  },
  "author": "WaterGenie35",
  "license": "ISC",
  "engines": {
    "npm": "10.5.0",
    "node": "v20.12.1"
  },
  "dependencies": {
    "@prisma/client": "5.12.1",
    "compression": "1.7.4",
    "express": "4.19.2",
    "helmet": "7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0",
    "@faker-js/faker": "8.4.1",
    "@stylistic/eslint-plugin": "1.7.0",
    "@types/compression": "1.7.5",
    "@types/debug": "4.1.12",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "debug": "4.3.4",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.3",
    "globals": "15.0.0",
    "prisma": "5.12.1"
  }
}
